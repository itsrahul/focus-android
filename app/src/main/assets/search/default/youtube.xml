<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
<ShortName>Youtube</ShortName>
<Image width="16" height="16">data:image/png;base64,</Image>
<Url type="application/x-suggestions+json" template="https://www.youtube.com/osjson.aspx">
  <Param name="query" value="{searchTerms}"/>
  <Param name="language" value="{moz:locale}"/>
</Url>
<Url type="text/html" method="GET" template="https://www.youtube.com/search">
  <Param name="q" value="{searchTerms}" />
  <Param name="pc" value="MOZB" />
  <Param name="form" value="MOZMBA" />
</Url>
<SearchForm>https://www.youtube.com</SearchForm>
</SearchPlugin>